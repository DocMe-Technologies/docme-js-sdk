{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "doc": "typedoc",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "socket.io-client": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@docme/docme-js-sdk",
  "author": "Muamet Islami",
  "module": "dist/docme-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/docme-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/docme-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "@types/dom-mediacapture-record": "^1.0.11",
    "@types/url-join": "^4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^7.0.4",
    "jest-fetch-mock": "^3.0.3",
    "semantic-release": "^18.0.1",
    "size-limit": "^6.0.4",
    "socket.io-client": "^4.4.0",
    "socket.io-mock": "^1.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4",
    "file-type": "^16.5.3",
    "form-data": "^4.0.0",
    "get-blob-duration": "^1.2.0",
    "p-retry": "4.6.1",
    "url-join": "^4.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
